{
	"name": "fuzzy-chainsaw",
	"version": "1.0.0",
	"description": "Boilerplate for component-based UI development",
	"repository": "connectivedx/fuzzy-chainsaw",
	"bugs": {
		"url": "https://github.com/connectivedx/fuzzy-chainsaw/issues"
	},
	"author": "Connective DX",
	"license": "MIT",
	"directories": {
		"source": "./source",
		"dest": "./dist"
	},
	"scripts": {
		"postinstall": "npm run dll",
		"start": "http-server ./dist -p 8081",
		"dll": "webpack --config ./build/webpack/webpack.config.dll.js --progress",
		"dll:production": "webpack --config ./build/webpack/webpack.config.dll.production.js --progress",
		"dev": "gulp clean:pre && npm run dev:webpack & npm run dev:test",
		"dev:webpack": "webpack-dev-server --config ./build/webpack/webpack.config.dev.js --progress --inline",
		"dev:test": "karma start --auto-watch",
		"build": "gulp build",
		"watch": "gulp watch",
		"production": "gulp production:static",
		"production:ci": "gulp production:ci",
		"test": "gulp test:ci",
		"new:tag": "gulp scaffold:tag --name",
		"new:component": "gulp scaffold:component --name"
	},
	"engines": {
		"node": "^=6.9.2"
	},
	"devDependencies": {
